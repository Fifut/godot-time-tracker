; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Project Time Tracker"
run/main_scene="res://addons/project-time-tracker/TrackerDock.tscn"
config/features=PackedStringArray("4.5")
config/icon="res://addons/project-time-tracker/icon.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/project-time-tracker/plugin.cfg")

[project_time_traker]

sections/show_sections=true
sections/show_graphs=true
afk/afk_timer=10

[rendering]

environment/default_environment="res://default_env.tres"
